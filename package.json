{
  "name": "umbraco-cypress-testhelpers",
  "version": "1.0.0",
  "description": "Test helpers for making Cypress tests for Umbraco solutions",
  "main": "./lib/index.js",
  "types": "lib/types",
  "scripts": {
    "clean": "rimraf lib",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint --project ./tsconfig.json",
    "test:jest": "jest --clearCache && jest --reporters=jest-trx-results-processor --coverage --coverageReporters=cobertura ",
    "test": "npm run test:jest",
    "build": "tsc",
    "prepare": "npm run clean && npm run lint && npm run format && npm run test && npm run build",
    "cypress:ui": "start-server-and-test ci:serve http://localhost:9000 cypress:open",
    "cypress:ci": "start-server-and-test ci:serve http://localhost:9000 cypress:run",
    "cypress:open": "cypress open  --config integrationFolder=cypress/integration",
    "cypress:run": "cypress run  --config integrationFolder=cypress/integration",
    "ci:serve": "node iis-express.js",
    "postinstall": "cross-env-shell node postinstall.js $INIT_CWD"
  },
  "files": [
    "lib",
    "src",
    "postinstall.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bergmania/umbraco-cypress-testhelpers.git"
  },
  "keywords": [
    "umbraco",
    "cypress",
    "builder",
    "pattern",
    "e2e",
    "test",
    "cypress-plugin"
  ],
  "author": "Bjarke Berg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bergmania/umbraco-cypress-testhelpers/issues"
  },
  "homepage": "https://github.com/bergmania/umbraco-cypress-testhelpers",
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@types/chai": "^4.2.11",
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.12.2",
    "camelize": "^1.0.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.0.2",
    "faker": "^4.1.0",
    "jest": "^25.3.0",
    "jest-trx-results-processor": "^2.0.0",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "start-server-and-test": "^1.10.11",
    "testdouble": "^3.12.5",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {},
  "peerDependencies": {
    "cypress": "^4.0.2",
    "cross-env": "^7.0.2",
    "rxjs": "^6.5.5",
    "ncp": "^2.0.0"
  }
}
